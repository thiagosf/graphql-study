{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-shorthand": false,
    "object-literal-sort-keys": [
      false
    ],
    "ter-indent": [
      true,
      2
    ],
    "no-console": false,
    "only-arrow-functions": [
      false
    ],
    "member-access": false,
    "max-classes-per-file": [
      true,
      5
    ],
    "no-shadowed-variable": false,
    "interface-name": [
      false
    ],
    "max-line-length": [
      true,
      200
    ],
    "no-var-requires": false
  },
  "rulesDirectory": []
}
